#include <stdio.h>

int main()

{
    int sales,p=0;
    float salary;
    
        while(p<5)
        {
            printf("Enter the sales number of product A: ");
            scanf("%d",&sales);
            
            if(sales<10)
            {
                printf("Salary: $2000.00\n");
                p++;
            }
            else if(9<sales && sales<100)
            {
                salary = 2000 + 12.5*sales;
                printf("Salary: $%.2f\n",salary);
                p++;
            }
            else
            {
                salary = 2000 + sales*125*15/100;
                printf("Salary: $%.2f\n",salary);
                p++;
            }
            
        }

    return 0;
}
// bitti :D.
